@import 'variables.styl'

html, body
  font-family fontFamily
  background  bgColor
  color txtColor
  font-size 16px

html,body,ul,h1,h2,h3,h4,h5,h6
  padding 0
  margin 0

h1, h2
  margin 1em 0

a
  text-decoration none
  font-weight bold
  color brand
  &:hover
    color hoverColor


img
  max-width 100%